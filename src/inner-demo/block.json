{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "acf-block-starter/inner-demo",
	"version": "0.1.0",
	"title": "Inner Demo",
	"category": "acf-block-starter",
	"icon": "columns",
	"description": "Demonstration of inner blocks in ACF",
	"example": {
	},
	"acf": {
		"mode": "preview",
		"renderTemplate": "render.php"
	},
	"supports": {
		"html": false,
		"anchor": false,
		"jsx": true,
		"mode": false,
		"spacing": {
			"margin": true,
			"padding": true,
			"blockGap": true
		},
		"color": {
			"text": true,
			"background": true
		}
	},
	"attributes": {
		"align": {
			"type": "string",
			"default": "full"
		}
	},
	"textdomain": "acf-block-starter",
	"script": "file:./index.js"
}
